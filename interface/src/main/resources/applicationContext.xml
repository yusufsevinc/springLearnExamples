<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="mysql" class="com.spring.interface2.interface2.db.impl.MySql" destroy-method="closeConnection" lazy-init="true"></bean>

    <bean id="oracle" class="com.spring.interface2.interface2.db.impl.Oracle" destroy-method="closeConnection" lazy-init="true"></bean>
</beans>